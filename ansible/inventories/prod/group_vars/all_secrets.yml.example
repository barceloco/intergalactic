---
# Secrets file template
# Copy this file to all_secrets.yml (which is gitignored) and fill in your actual values
#
# Usage:
#   cp all_secrets.yml.example all_secrets.yml
#   # Edit all_secrets.yml with your actual keys and secrets

# SSH public keys for the automation user (ansible)
automation_authorized_keys:
  - "ssh-ed25519 AAAAC3NzaC1lZDI1NTE5AAAAI... ansible@control"
  # Add additional keys as needed

# Human user accounts with their SSH public keys
human_users:
  - name: armand
    authorized_keys:
      - "ssh-ed25519 AAAAC3NzaC1lZDI1NTE5AAAAI... user@laptop"
      # Add additional keys as needed

# Tailscale authentication key (one-time use key from Tailscale admin console)
tailscale_authkey: "tskey-auth-..."

